    "class Solution:\t\n",
    "\tdef binarysearch(arr, l, r, x):\n",
    "\t\tif r >= l:\n",
    "\t\t    mid = l+(r-l)//2\n",
    "\t\t    if arr[mid] == x:\n",
    "\t\t        return mid\n",
    "\t\t    elif arr[mid] > x:\n",
    "\t\t        return binarySearch(arr, mid+1, r, x)\n",
    "\t\t    else:\n",
    "\t\t        return-1\n",
    "\t\t        "
